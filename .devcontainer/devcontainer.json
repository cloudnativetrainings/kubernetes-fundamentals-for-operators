{
  "name": "Kubernetes Fundamentals For Operators Training Labs",
  "image": "quay.io/kubermatic-labs/training-ghcs-kubernetes-fundamentals-for-operators-trainee-environment:1.0.7",
  "customizations": {
    "vscode": {
      "settings": {
        "workbench.editorAssociations": {
          "*.md": "vscode.markdown.preview.editor"
        },
        "terminal.integrated.shell.linux": "/bin/bash",
        "files.exclude": {
          ".git/": true,
          ".gititnore": true,
          ".devcontainer/": true,
          ".99_todos/": true,
          "teardown.sh": true,
          "makefile": true
        }
      }
    }
  },
  "remoteUser": "root"
}